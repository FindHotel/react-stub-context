{
  "name": "react-stub-context",
  "version": "0.1.4",
  "description": "Stub context for a component, to be used for testing purposes.",
  "main": "dist/index.js",
  "keywords": [
    "react",
    "test",
    "stub",
    "context",
    "mock"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/karlbright/react-stub-context.git"
  },
  "bugs": {
    "url": "https://github.com/karlbright/react-stub-context/issues"
  },
  "scripts": {
    "test": "jest",
    "prepublish": "browserify --no-bundle-external -s stubContext --bare src/index.js -o dist/index.js"
  },
  "author": "Karl Brightman <karl@karlbright.org>",
  "license": "ISC",
  "devDependencies": {
    "babel-jest": "^5.2.0",
    "babel-loader": "^5.1.3",
    "babel-plugin-object-assign": "^1.1.0",
    "babelify": "^6.1.1",
    "browserify": "^10.2.1",
    "jest-cli": "^0.4.5",
    "react": "^0.13.3",
    "uglifyify": "^3.0.1",
    "webpack": "^1.9.9"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "browserify": {
    "transform": [ "babelify", "uglifyify" ]
  }
}
